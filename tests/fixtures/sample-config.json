{
  "targets": [
    {
      "name": "test-target",
      "path": "tests/fixtures/target.json",
      "key": "mcpServers"
    }
  ],
  "mcpServers": {
    "filesystem": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-filesystem", "${ROOT}"]
    },
    "github": {
      "command": "npx", 
      "args": ["-y", "@modelcontextprotocol/server-github"],
      "env": {
        "GITHUB_PERSONAL_ACCESS_TOKEN": "${TOKEN}"
      }
    }
  }
}